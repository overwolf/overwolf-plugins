{
  "manifest_version":"1",
  "type":"WebApp",
  "meta":{
    "name":".Net SimpleIO Plugin tester App",
    "version":"1.0.0.0",
    "minimum-overwolf-version": "0.85.0.0",
    "author":"Overwolf",
    "icon":"icon.png",
    "icon_gray":"icon_gray.png",
    "description":".Net SimpleIO Plugin tester App"
  },
  "permissions": ["Extensions"],
  "data": {
    "start_window":"index",	  
    "windows":{
      "index":{
        "file":"index.html",
        "transparent":false,
        "resizable":true,
        "show_in_taskbar": true,
        "size": {
          "width": 400,
          "height": 400
        },
        "start_position": {
          "Top": 100,
          "Left": 300
        }
      }
    },	
	"extra-objects": {
      "simple-io-plugin": {
        "file": "simple-io-plugin.dll",
        "class": "overwolf.plugins.simpleio.SimpleIOPlugin"
      }
    }
  }
}
